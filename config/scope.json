{
  "engagement_name": "Example Penetration Test",
  "targets": [
    "192.168.1.0/24",
    "testapp.example.com"
  ],
  "excluded": [
    "192.168.1.1",
    "192.168.1.254"
  ],
  "rules_of_engagement": {
    "max_concurrent_scans": 5,
    "rate_limit_delay": 1.0,
    "stealth_mode": false,
